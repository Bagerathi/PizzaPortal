    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>8d287b1c-4e33-4378-8ba8-62d5f551adec</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
